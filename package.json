{
  "name": "common-utils-pkg",
  "version": "1.5.1",
  "description": "A package of commonly used utilities",
  "main": "./lib/common-utils-pkg.min.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "4.1.6",
    "babel-preset-env": "1.7.0",
    "chai": "4.1.2",
    "codecov": "3.0.4",
    "cross-env": "5.2.0",
    "documentation": "8.0.1",
    "eslint": "5.0.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.13.0",
    "husky": "0.14.3",
    "istanbul": "0.4.5",
    "lint-staged": "7.2.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8",
    "yargs": "12.0.1"
  },
  "dependencies": {},
  "scripts": {
    "build:clean": "rimraf lib",
    "build": "npm run build:clean && webpack --env dev && webpack --env build && cp -r src/ts/* lib/",
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text node_modules/mocha/bin/_mocha src/**/*.spec.js && node_modules/codecov/bin/codecov",
    "docu": "node_modules/documentation/bin/documentation.js build src/index.js -f json > docs/source.json && node scripts/documentation.js",
    "lint:install": "npm i lint-staged husky",
    "lint": "node_modules/eslint/bin/eslint.js src/",
    "precommit": "lint-staged",
    "prepare": "npm run lint && npm run cover && npm run build",
    "test:watch": "mocha --require babel-core/register --colors -w src/**/*.spec.js",
    "test": "mocha --require babel-core/register --colors src/**/*.spec.js && eslint src/"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run cover",
      "git add"
    ]
  },
  "nyc": {
    "register": "babel-register",
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/codesandcoffees/common-utils-pkg.git"
  },
  "keywords": [
    "utilities",
    "npm",
    "module",
    "javascript",
    "es6",
    "functions"
  ],
  "author": "iamdevlinph@codes-and-coffees",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codesandcoffees/common-utils-pkg/issues"
  },
  "homepage": "https://github.com/codesandcoffees/common-utils-pkg#readme",
  "engines": {
    "node": "8.11.3"
  }
}
